{
  "name": "tslint-action",
  "version": "0.1.1",
  "private": true,
  "description": "Lint typescript with annotations support",
  "main": "dist/main.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "lint": "eslint **/*.ts",
    "test": "jest",
    "all": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gldmtr/tslint-actions.git"
  },
  "keywords": [
    "tslint",
    "github",
    "action"
  ],
  "author": "gldmtr",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "common-tags": "^1.8.0",
    "glob": "^7.1.6",
    "tslint": "^6.1.3",
    "typescript": "^4.2.2"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.0",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^3.14.0",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4"
  }
}
